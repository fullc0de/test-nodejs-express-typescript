[{
    "name": "postgresdb",
    "type": "postgres",
    "url": "{use $POSTGRES_URL env}",
    "entities": [
        "build/model/**/*.js"
    ],
    "migrations": [
        "build/migration/**/*.js"
    ],
    "synchronize": false,
    "logging": ["query", "error", "schema"]
}, {
    "name": "mysqldb",
    "type": "mysql",
    "url": "use $MYSQL_URL env"
}]
